#!/bin/bash

errorString=""


if [ ${#1} -lt 10 ];then 
errorString="$errorString we have less than 10 charactere"
fi

if [[ ! $1 =~  [1-9] ]];then
errorString="$errorString /n il y pas un chiffre"
fi

if [[ ! $1 =~  [a-z] ]];then
errorString=" $errorString /n il y pas de petit char"
fi

if [[ ! $1 =~  [A-Z] ]];then
errorString="$errorString il y pas de grand char"
fi

echo $errorString
